public OnPlayerEnterVehicle(playerid, vehicleid, ispassenger)
{
	if(FormaFrac[playerid] == 1)
	{
		foreach(new i:Player)
		{
		    if(gotome[i] == playerid)
		    {
		        ClearAnimations(playerid);
		        SendClientMessage(playerid, COLOR_GREY, "Tqven Miatrevt Damnashaves Da Ver Chajdebit Manqanashi, Jer Daweret: /gotome [ID Motamashis]");
		        return true;
			}
		}
	}
	if(IsPlayerInAnyVehicle(playerid)) return TrollDetect(playerid);
    if(IsLocked[vehicleid] == 1)
	{
		ClearAnimations(playerid);
	}
	if(IsLocked[vehicleid] == 1)
	{
	    LockCar(vehicleid);
	//	GameTextForPlayer(playerid,"~w~Manqana ~r~Daketilia", 3000, 3);
		ClearAnim(playerid);
	}
	if(!ispassenger)
	{
		if(IsVehicleHasDriver(vehicleid))
		{
			SendClientMessage(playerid,-1,"Motamashis transportidan gadmogdeba akrdzalulia!");
			new Float:X,Float:Y,Float:Z;
			GetPlayerPos(playerid,X,Y,Z);
			SetPlayerPos(playerid,X+0.1,Y+0.1,Z+0.1);
			SetPVarInt(playerid, "AntiBreik", 1);
			ClearAnimations(playerid);
			return 1;
		}
		if(caridhouse[playerid] != vehicleid)
		{
			new vehowner = GetVehicleOwner(vehicleid);
			if(vehowner != INVALID_PLAYER_ID)
			{
			    if(keyfloods[playerid] == false)
			    {
			        keyfloods[playerid] = true;
					new exQuery[350],name[28],nametarget[28];
					GetPlayerName(playerid,name,28);
					GetPlayerName(vehowner,nametarget,28);
					mysql_format(connects,exQuery,350,"SELECT `Owner`,`Player` FROM `carkeys` WHERE `Owner` = '%s' AND `Player` = '%s'",nametarget,name);
					mysql_tquery(connects,exQuery,"CheckForKey","ii",playerid,vehowner);
					SetTimerEx("keyfloodoff", 5000,false,"d",playerid);
				}
				else if(keyfloods[playerid] == true)
				{
				    ClearAnimations(playerid);
					new Float:X,Float:Y,Float:Z;
					GetPlayerPos(playerid,X,Y,Z);
					SetPlayerPos(playerid,X,Y,Z);
					SetPVarInt(playerid, "AntiBreik", 1);
				}
			}
		}
	}
	if(usemats[playerid] > 0)
	{
	    if(usemats[playerid] == 1) armmatsf += 250;
	    if(usemats[playerid] == 2) lsamatbi += 250;
	    if(usemats[playerid] == 3) armmatbi += 250;
	    usemats[playerid] = 0;
		RemovePlayerAttachedObject(playerid, 1);
		SendClientMessage(playerid, COLOR_REDD,"Tqven Dagivardat Yuti");
		SetPlayerSpecialAction (playerid, SPECIAL_ACTION_NONE);
	}
	if(!ispassenger) vehicleidtp[playerid] = vehicleid;
	SetVehicleParamsForPlayer(vehicleid, playerid, 0, IsLocked[vehicleid]);
	Refueling[playerid] = 0;
	return true;
}
