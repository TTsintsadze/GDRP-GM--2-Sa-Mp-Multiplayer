CMD:changeboat(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(PlayerInfo[playerid][pBoat] == 0) return errortxt(playerid, "Tkven Ar Gyavt Shenadzeni Kateri!");
	if(BoatEnter[playerid] == true) return errortxt(playerid, "Tkven Ukve Shestavazet Motamashes Kateris Yidva, Gasaukmeblad Sheiyvanet: /clearboat");
	if(sscanf(params, "ud", params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /changeboat [Motamashis ID] [Tanxa]");
	if(PlayerInfo[params[0]][pBoat] != 0) return infotxt(playerid, "Am Motamashes Ukve Yavs Kateri!");
	if(params[0] == playerid) return infotxt(playerid, "Tkven Miutitet Tkveni ID!");
	if(!ProxDetectorS(10.0, playerid, params[0])) return errortxt(playerid, "Motamashe Ar Imyofeba Tkventan Axlos");
	if(params[1] < 30000000 || params[1] > 100000000) return infotxt(playerid, "Minimaluri Fasi: 30KK / Maxsimaluri Fasi: 100KK!");
	new boatisid;
	if(boatid[playerid] != -1) boatisid = boatid[playerid];
	new Float:cx,Float:cy,Float:cz;
	GetVehiclePos(boatisid, cx, cy, cz);
	if(!PlayerToPoint(4.0, playerid, cx, cy, cz)) return errortxt(playerid, "Tkven Ar Imyofebit Katertan Axlos!");
	BoatMyidveli[params[0]] = playerid;
	BoatGamyidveli[playerid] = params[0];
	BoatFasi[params[0]] = params[1];
	BoatEnter[playerid] = true;
	new changeboatstr[300];
	format(changeboatstr, sizeof(changeboatstr), "{FFFFFF}Tkven Shestavazet {63BD4E}%s{FFFFFF}'is Yidva Motamashe: {63BD4E}%s{FFFFFF}'s", BoatName(playerid), Name(params[0]));
	SendClientMessage(playerid, COLOR_WHITE, changeboatstr);
	format(changeboatstr, sizeof(changeboatstr), "{FFFFFF}Fasi: {63BD4E}%d$", params[1]);
	SendClientMessage(playerid, COLOR_WHITE, changeboatstr);

	format(changeboatstr, sizeof(changeboatstr), "{63BD4E}%s{FFFFFF}'m(a) Shemogtavazat {63BD4E}%s{FFFFFF}'is Yidva", Name(playerid), BoatName(playerid));
	SendClientMessage(params[0], COLOR_WHITE, changeboatstr);
	format(changeboatstr, sizeof(changeboatstr), "{FFFFFF}Fasi: {63BD4E}%d$ {FFFFFF}(/accept boat)", params[1]);
	SendClientMessage(params[0], COLOR_WHITE, changeboatstr);
	return true;
}
