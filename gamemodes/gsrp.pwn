	    {
	        format(string, sizeof(string), "{38AF91}[Buisness Info] {FFFFFF}Tkveni Biznesi Ukve: {38AF91}%i{FFFFFF}Saatia Daketilia",BizzInfo[GetPVarInt(i, "PlayerBizz")][bLockTime]);
