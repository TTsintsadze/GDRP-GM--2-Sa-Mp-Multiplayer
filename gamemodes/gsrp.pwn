stock SetPlayerCriminal(playerid,forplayerid,const reason[])
{
	new string[356];
	PlayerInfo[forplayerid][pWantedLevel] += 1;
	SetPlayerWantedLevel(forplayerid, PlayerInfo[forplayerid][pWantedLevel]);
	format(string,sizeof(string),"Officer: %s'm(a) Gamogacxadat Dzebnilad. Mizezi: %s", Name(playerid), reason);
	SendClientMessage(forplayerid, COLOR_YELLOW, string);
	format(string,sizeof(string),"Tkveni Dzebnis Done Aris: %d", PlayerInfo[forplayerid][pWantedLevel]);
	SendClientMessage(forplayerid, COLOR_YELLOW, string);
	format(string, sizeof(string), "Racia: Danashauli: %s. Damnashave: %s. Dzebnis Done: %d | Gvatyobinebs: %s", reason, Name(forplayerid), PlayerInfo[forplayerid][pWantedLevel], Name(playerid));
	SendCopsMessage(COLOR_YELLOW,string);
	PlayerUpdateData(forplayerid, "pWantedLevel", PlayerInfo[forplayerid][pWantedLevel]);
