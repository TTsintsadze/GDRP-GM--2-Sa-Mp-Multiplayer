CMD:givevip(playerid, params[])
{
	new ip[MAX_PLAYER_NAME];
	GetPlayerIp(playerid,ip,sizeof(ip));
	if(Mtavari(playerid) && PlayerInfo[playerid][pAdmin] >= 9)
	{
		if(PlayerInfo[playerid][pLogin] == 0) return true;
		if(PlayerInfo[playerid][pAdmin] < 9) return true;
		if(!IsPlayerConnected(playerid)) return true;
		if(PlayerInfo[playerid][pPrivilegia] < 2) return SendClientMessage(playerid, COLOR_WHITE, "{FF0000}[Erorr] {FFFFFF}Tkven Argaqvt Privilegia");
