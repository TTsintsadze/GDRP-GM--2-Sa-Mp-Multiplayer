		{
			case 1: format(string, sizeof(string), "{38AF91}[LSPD] {FFFFFF}Am Saatshi Tkven Gamoimushavet: {38AF91}%d$", checks);
			case 2: format(string, sizeof(string), "{38AF91}[FBI] {FFFFFF}Am Saatshi Tkven Gamoimushavet: {38AF91}%d$", checks);
			case 3: format(string, sizeof(string), "{38AF91}[ARMY] {FFFFFF}Am Saatshi Tkven Gamoimushavet: {38AF91}%d$", checks);
