			case 5,6,14: format(string, sizeof(string), "{38AF91}[Maffia] {FFFFFF}Am Saatshi Tkven Gamoimushavet: {38AF91}%d$", checks);
			case 12,13,15,17,18: format(string, sizeof(string), "{38AF91}[Ghetto] {FFFFFF}Am Saatshi Tkven Gamoimushavet: {38AF91}%d$", checks);
