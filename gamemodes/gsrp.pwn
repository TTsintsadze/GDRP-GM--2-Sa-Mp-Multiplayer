publics: SetPlayerWeapons(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		if(PlayerInfo[playerid][pJailed] < 1)
		{
			if(IsABiker(playerid))
			{
				GivePlayerServerWeapon(playerid, 7, 1);
			}
			if(FormaFrac[playerid] == 5)
			{
				GivePlayerServerWeapon(playerid, 2, 1);
			}
			if(FormaFrac[playerid] == 6)
			{
				GivePlayerServerWeapon(playerid, 8, 1);
			}
			if(FormaFrac[playerid] == 7)
			{
				if(PlayerInfo[playerid][pRank] == 4 || PlayerInfo[playerid][pRank] == 3)
				{
					GivePlayerServerWeapon(playerid, 24, 200);
					SetPlayerArmourAC(playerid, 100);
				}
			}
			if(FormaFrac[playerid] == 14)
			{
				GivePlayerServerWeapon(playerid, 1, 1);
			}
			if(FormaFrac[playerid] == 16)
			{
				GivePlayerServerWeapon(playerid, 43, 30);
			}
			if(PlayerInfo[playerid][pMember] == 26)
			{
				GivePlayerServerWeapon(playerid, 5, 1);
			}
			if(PlayerInfo[playerid][pMember] == 24)
			{
				GivePlayerServerWeapon(playerid, 24, 250);
				GivePlayerServerWeapon(playerid, 30, 500);
			}
		}
	}
	return true;
}
