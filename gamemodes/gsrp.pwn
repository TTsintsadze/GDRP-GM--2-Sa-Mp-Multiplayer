		{
			PlayerInfo[params[0]][pDonateRank] = 0;
			PlayerInfo[params[0]][pVipTime] = 0;
		}
		format(YCMDstr, sizeof(YCMDstr), "{F78181}Administratorma %s Misca %s's - {FF0000}[VIP LVL]:{F78181} %d. %d - Dgit",Name(playerid),Name(params[0]),params[1], params[2]);
