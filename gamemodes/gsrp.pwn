public SV_VOID:OnPlayerActivationKeyPress(
	SV_UINT:playerid,
	SV_UINT:keyid
) {
	if (keyid == 0x42 && lstream[playerid]) SvAttachSpeakerToStream(lstream[playerid], playerid);
}

public SV_VOID:OnPlayerActivationKeyRelease(
	SV_UINT:playerid,
	SV_UINT:keyid
) {
	if (keyid == 0x42 && lstream[playerid]) SvDetachSpeakerFromStream(lstream[playerid], playerid);
}
@FamkaName(playerid, inputtext[]);
@FamkaName(playerid, inputtext[])
{
    new rows;
	cache_get_row_count(rows);
	if(rows)
	{
	    infotxt(playerid, "Tkvens Mier Mititebuli Saxeli Ukve Arsebobs Monacemta Bazashi");
		return false;
	}
	if(PlayerInfo[playerid][pCash] < 2500000) return errortxt(playerid, "Tkven Ar Gakvt Sakmarisi Tanxa!");
	PlayerInfo[playerid][pCash] -= 2500000;
	PlayerUpdateData(playerid, "pCash", PlayerInfo[playerid][pCash]);
	strmid(FamilyInfo[PlayerInfo[playerid][pFamilyMember]][famName],inputtext, 0, strlen(inputtext), 20);
	UpdateFamilyStr(PlayerInfo[playerid][pFamilyMember], "name", FamilyInfo[PlayerInfo[playerid][pFamilyMember]][famName]);

	PC_EmulateCommand(playerid, "/fmenu");

	foreach(new i: Player)
	{
		if(PlayerInfo[i][pFamilyText] != Text3D:INVALID_3DTEXT_ID)
		{
			if(PlayerInfo[playerid][pFamilyMember] == PlayerInfo[i][pFamilyMember])
			{
			    new textstr[150];
			    format(textstr,sizeof(textstr),"{%s}%s {FFFFFF}%s", family_chat_color[FamilyInfo[PlayerInfo[i][pFamilyMember]][famChatColor]], FamTypes[FamilyInfo[PlayerInfo[playerid][pFamilyMember]][fType]], FamilyInfo[PlayerInfo[i][pFamilyMember]][famName]);
				UpdateDynamic3DTextLabelText(PlayerInfo[i][pFamilyText], -1, textstr);
			}
		}
		else if(PlayerInfo[i][pFamilyText] == Text3D:INVALID_3DTEXT_ID)
		{
			if(PlayerInfo[playerid][pFamilyMember] == PlayerInfo[i][pFamilyMember])
			{
			    new textstr[150];
			    format(textstr,sizeof(textstr),"{%s}%s{FFFFFF} %s", family_chat_color[FamilyInfo[PlayerInfo[i][pFamilyMember]][famChatColor]], FamTypes[FamilyInfo[PlayerInfo[playerid][pFamilyMember]][fType]], FamilyInfo[PlayerInfo[i][pFamilyMember]][famName]);//??? ???
				PlayerInfo[i][pFamilyText] = CreateDynamic3DTextLabel(textstr, -1, 0.0, 0.0, 0.4, 6.0,  i, INVALID_VEHICLE_ID, 1 );
			}
		}
	}
	return true;
}
