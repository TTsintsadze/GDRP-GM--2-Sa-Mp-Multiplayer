publics: GzCheck()
{
	for(new i = 0; i < sizeof(GZInfo); i++)
	{
		if(ZoneOnBattle[i] == 1)
		{
			if(GZSafeTime[i] > 0) GZSafeTime[i] --;
            foreach(new x:Player)
 			{
				new captinfo[150];
				new y = PlayerInfo[x][pMember];
				if(y == GZInfo[i][gNapad] || y == GZInfo[i][gFrakVlad])
				{
					if(CaptureOn[x] == 0)
					{
					    switch(GZInfo[i][gNapad])
						{
							case 12:
							{
								PlayerTextDrawColor(x,CaptTXT_P8[x], -16711681);
							}
							case 13:
							{
								PlayerTextDrawColor(x,CaptTXT_P8[x], -65281);
							}
							case 15:
							{
								PlayerTextDrawColor(x,CaptTXT_P8[x], 8388863);
							}
							case 17:
							{
								PlayerTextDrawColor(x,CaptTXT_P8[x], 16777215);
							}
							case 18:
							{
								PlayerTextDrawColor(x,CaptTXT_P8[x], 8454143);
							}
						}
						switch(GZInfo[i][gFrakVlad])
						{
							case 12:
							{
								PlayerTextDrawColor(x,CaptTXT_P7[x], -16711681);
							}
							case 13:
							{
								PlayerTextDrawColor(x,CaptTXT_P7[x], -65281);
							}
							case 15:
							{
								PlayerTextDrawColor(x,CaptTXT_P7[x], 8388863);
							}
							case 17:
							{
								PlayerTextDrawColor(x,CaptTXT_P7[x], 16777215);
							}
							case 18:
							{
								PlayerTextDrawColor(x,CaptTXT_P7[x], 8454143);
				            }
				        }
				        for(new t; t != 13; t++) TextDrawShowForPlayer(x, CaptTXT[t]);
				        for(new p; p != 7; p++) PlayerTextDrawShow(x, CaptTXT_P[x][p]);
				        PlayerTextDrawShow(x, CaptTXT_P8[x]);
						PlayerTextDrawShow(x, CaptTXT_P7[x]);
						CaptureOn[x] = 1;
					}
					if(GZSafeTime[i] > 0) format(captinfo,sizeof(captinfo),"%s",Convert(GZSafeTime[i]));
					PlayerTextDrawSetString(x, CaptTXT_P[x][6], captinfo);
					format(captinfo,sizeof(captinfo),"%d", CountOnZone[GZInfo[i][gFrakVlad]]);
					PlayerTextDrawSetString(x, CaptTXT_P[x][4], captinfo);
					format(captinfo,sizeof(captinfo),"%d", CountOnZone[GZInfo[i][gNapad]]);
					PlayerTextDrawSetString(x, CaptTXT_P[x][5], captinfo);
					new highscore = -1;
					new highscoreowner = INVALID_PLAYER_ID;
					foreach(new p:Player)
					{
					    new g = PlayerInfo[p][pMember];
						if(g == GZInfo[i][gNapad] || g == GZInfo[i][gFrakVlad])
						{
							if(KillCounter[p] > highscore)
							{
								highscore = KillCounter[p];
								highscoreowner = p;
							}
						}
					}
					format(captinfo,sizeof(captinfo),"%s", Name(highscoreowner));
					PlayerTextDrawSetString(x, CaptTXT_P[x][1], captinfo);
				}
			}
