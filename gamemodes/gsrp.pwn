public OnPlayerDisconnect(playerid, reason)
{
    OnPlayerUpdateAc(playerid);
	if(GetPlayerState(playerid) == PLAYER_STATE_PASSENGER)
	{
		foreach(new i:Player)
		{
			if(IsPlayerInVehicle(i, GetPlayerVehicleID(playerid)) && GetPlayerState(i) == 2 && TransportDuty[i] == 1)
			{
				if(GetPVarInt(i,"Taxi_")-1 == playerid)
				{
					SendClientMessage(i, COLOR_BLUE, "Tkvenma Mgzavrma Datova Transporti, Tanxa Dagericxebat Payday'ze");
					if(GetPVarInt(i,"PayDayCashJob") > gettime()) {}
					else
					{
						new taxizp = 200+random(200);
						PlayerInfo[i][pPayCheck] += taxizp*PlayerInfo[i][ptaxilvl];
					}
					DeletePVar(i,"Taxi_");
					for(new id; id < GetMaxPlayers(); id++)
					{
						if(IsPlayerInVehicle(id, GetPlayerVehicleID(playerid)) && id != i && id != playerid)
						{
						    new taxistr4[180];
							format(taxistr4, sizeof(taxistr4), "Miiyvanet Mgzavri: %s Danishnulebis Adgilas Da Xelfasi Payday'ze Dagericxebat",Name(id));
							SendClientMessage(i, COLOR_BLUE, taxistr4);
							SetPVarInt(i,"Taxi_",id+1);
							break;
						}
					}
				}
			}
		}
	}
	if(GetPVarType(playerid, "PlacedBB"))
    {
        DestroyDynamicObject(GetPVarInt(playerid, "PlacedBB"));
        DestroyDynamic3DTextLabel(Text3D:GetPVarInt(playerid, "BBLabel"));
        if(GetPVarType(playerid, "BBArea"))
        {
            foreach(Player,i)
            {
                if(IsPlayerInDynamicArea(i, GetPVarInt(playerid, "BBArea")))
                {
                    StopAudioStreamForPlayer(i);
                    SendClientMessage(i, COLOR_LIGHTBLUE, " Boombox mepatronem datova serveri.");
                }
            }
        }
	}
	if(PlayerInfo[playerid][pFamilyText] != Text3D:INVALID_3DTEXT_ID)
	{
		DestroyDynamic3DTextLabel(PlayerInfo[playerid][pFamilyText]);
		PlayerInfo[playerid][pFamilyText] = Text3D:INVALID_3DTEXT_ID ;
	}
	if(boatid[playerid] != -1)
	{
		DestroyVehicle(boatid[playerid]);
		boatid[playerid] = -1;
	}
	/* RENT CAR */
	DestroyVehicle(Player_RentCar[playerid]);
	BoomBox[playerid] = 0;
	Player_RentCar[playerid] = -1;
	Opened_Rent_Car_Menu[playerid] = false;
	/*-------------------------------------------------------------------------*/
	if (lstream[playerid])
	{
		SvDeleteStream(lstream[playerid]);
		lstream[playerid] = SV_NULL;
	}
    if(speed_timer[playerid] != -1)
	{
		KillTimer(speed_timer[playerid]);
		speed_timer[playerid] = -1;
	}
	if(objjj[playerid] != -1)
	{
		DestroyDynamicObject(objjj[playerid]);
	}
	if(objjj1[playerid] != -1)
	{
		DestroyDynamicObject(objjj1[playerid]);
	}
	for(new k; k != 11; k++) PlayerTextDrawDestroy(playerid, MaffiaTXT[playerid][k]);
	new string[100];
	for(new i; i < 16; i++) PlayerTextDrawDestroy(playerid, speedometr[playerid][i]);
    KillTimer(chantis[playerid]);
    foreach(new i:Player)
	{
    	if(GetPlayerState(i) == PLAYER_STATE_SPECTATING && gSpectateID[i] == playerid)
    	{
        	StopSpectate(i);
       	 	SetPlayerHealth(i, 100);
        	GameTextForPlayer(i,"~w~RECON ~r~ OFF~n~ ~r~PLAYER DISCONNECT", 5000, 3);
    	}
	}
	if(creatcar[playerid] != -1)
	{
	 	DestroyVehicle(creatcar[playerid]);
 		creatcar[playerid] = -1;
	}
    if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && TransportDuty[playerid] == 1)
    {
        Delete3DTextLabel(taxi3d[GetPlayerVehicleID(playerid)]);
		TransportDuty[playerid] = 0;
	}
	SetPVarInt(playerid,"Register",0);
    new stringr[180];
	format(stringr, sizeof(stringr), "UPDATE accounts SET pLogin = '0' WHERE name = '%s'",Name(playerid));
	mysql_tquery(connects, stringr, "", "");
	new stolid = GetPVarInt(playerid,"GameStol");
	if(stolid != 88 )
	{
	    new g = KazinoDuty[playerid];
		TextDrawHideForPlayer(playerid,KostiBox[stolid]);
   		TextDrawHideForPlayer(playerid,TableName[stolid]);
   		TextDrawHideForPlayer(playerid,KostiInfo[stolid]);
   		TextDrawHideForPlayer(playerid,KostiGameInfo[stolid]);
   		TextDrawHideForPlayer(playerid,KostiKeyExit);
   		TextDrawHideForPlayer(playerid,KostiKeyDone);
   		TextDrawHideForPlayer(playerid,KostiKeyStavka);
   		SetPVarInt(playerid,"GameStol",88);
   		SetPVarInt(playerid,"StolReg",88);
   		TogglePlayerControllable(playerid,1);
   		CancelSelectTextDraw(playerid);
   		DelName(playerid,stolid);
   		SetPVarInt(playerid,"Ochki",88);
     	if(g > 0) SetTimerEx("NullStol",30000, false, "i", stolid), StolInfo[stolid][Stavka] = 0;
	}
    if(caridhouse[playerid] != -1)
	{
	    new bool:proverka;
     	foreach(new i:Player)
		{
			if(avtocar[i] == caridhouse[playerid]) proverka = true;
		}
		if(proverka == true) caroff[caridhouse[playerid]] = 1;
		else DestroyVehicle(caridhouse[playerid]);
		PlayerInfo[playerid][pProcents][0] = PlayerInfo[playerid][pProcents][0];
		PlayerInfo[playerid][pFuelcar] = Fuell[caridhouse[playerid]];
		caridhouse[playerid] = -1;
		createdcar --;
	}
	PlayerInfo[playerid][pForma] = FormaFrac[playerid];
	if(PlayerInfo[playerid][pJob] == 2 && gcontract[playerid] == 1) Delete3DTextLabel(Meh3d[GetPlayerVehicleID(playerid)]), gcontract[playerid] = 0;
	new sendername[MAX_PLAYER_NAME];
	new year, month,day;
	FormaFrac[playerid] = 0;
	format(WantNickChange[playerid],MAX_PLAYER_NAME, "");
	getdate(year, month, day);
	new hour, minute, second;
	gettime(hour, minute, second);
	GetPlayerName(playerid, sendername, sizeof(sendername));
	if(IsPlayerAttachedObjectSlotUsed(playerid,1)) RemovePlayerAttachedObject(playerid,1);
	/*-------------------------------------------------------------------------*/
	PlayerInfo[playerid][pDmArea] = 0;
	/*-------------------------------------------------------------------------*/
	if(PlayerInfo[playerid][pAdmin] > 0 && dostup[playerid] == 1)
	{
		dostup[playerid] = 0;
		format(string, sizeof(string), "Administrator: %s[%d] LVL: %d Disconnected",Name(playerid),playerid,PlayerInfo[playerid][pAdmin]);
		ABroadCast(COLOR_GREY, string, 1);
	}
	PicCP[playerid] = 0;
	enterbiz[playerid] = 0;
	arenda[playerid] = 0;
	keys[playerid] = 0;
	SetPlayerSkills(playerid);
	if(TransportCost[playerid] > 0 && TransportDriver[playerid] < 999)
	{
		if(IsPlayerConnected(TransportDriver[playerid]))
		{
			TransportMoney[TransportDriver[playerid]] += TransportCost[playerid];
			TransportTime[TransportDriver[playerid]] = 0;
			TransportCost[TransportDriver[playerid]] = 0;
			format(string, sizeof(string), "~g~+%d",TransportCost[playerid]);
			GameTextForPlayer(TransportDriver[playerid], string, 5000, 1);
		}
	}
	OnPlayerUpdateAc(playerid);
	PlayerInfo[playerid][pAdmin] = 0;
	foreach(new i:Player)
	{
	    if(gotome[i] == playerid)
	    {
	        gotome[i] = -1;
		}
		if(CarOffer[i] == playerid)
		{
		    CarOffer[i] = 999;
		}
		if(HouseOffer[i] == playerid)
		{
		    HouseOffer[i] = 999;
		}
	}
	if(BoatEnter[playerid] ==  true)
	{
		BoatMyidveli[BoatGamyidveli[playerid]] = 999;
		BoatGamyidveli[playerid] = 999;
		BoatEnter[playerid] = false;
	}
	if(PlayerCuffed[playerid] > 0)
	{
	    PlayerInfo[playerid][pJailed] = 1;
	    PlayerUpdateData(playerid, "pJailed", PlayerInfo[playerid][pJailed]);
		PlayerInfo[playerid][pWantedLevel] = 0;
		PlayerUpdateData(playerid, "pWantedLevel", PlayerInfo[playerid][pWantedLevel]);
		PlayerInfo[playerid][pJailTime] = 60*60;
		PlayerUpdateData(playerid, "pJailTime", PlayerInfo[playerid][pJailTime]);
		new cufstr[256];
		format(cufstr, sizeof(cufstr), "[Disconnect] Motamashe: %s Avtomaturad Chajda Cixeshi 1Saatit, Mizezi: Borkileb Dadebuli Gavida Serveridan!", Name(playerid));
    	SendClientMessageToAll(COLOR_LIGHTRED, cufstr);
	}
	if(tipsterplayer == playerid)
	{
	    SendRadioMessage(2,COLOR_ISPOLZUY ,"Motamashem Datova Raioni. Mosmena Shewyda");
		tipsteron = -1;
		tipsterplayer = -1;
	}
	bankent[playerid] = false;
	bankon[playerid] = false;
	xelsawyoebi[playerid] = false;
	RemovePlayerAttachedObject(playerid, 9);
	return true;
}
