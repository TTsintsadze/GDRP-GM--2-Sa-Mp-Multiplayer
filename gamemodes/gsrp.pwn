            format(fishstr, sizeof(fishstr), "{63BD4E}[Fishing System] {FFFFFF}Tkven Daichiret: {63BD4E}Oraguli {FFFFFF}/ {63BD4E}%d{FFFFFF}KG", fishkg*x2Boost);
            SendClientMessage(playerid, COLOR_WHITE, fishstr);

			SetPlayerAttachedObject(playerid, 8, 18632,6,0.079376,0.037070,0.007706,181.482910,0.000000,0.000000,1.000000,1.000000,1.000000);
            ApplyAnimation(playerid,"SWORD","sword_block",50.0,0,1,0,1,1);
            SetTimerEx("FishAnimation", 800, false, "i", playerid);

            format(fishstr, sizeof(fishstr), "Daichira: %dKG Oraguli", fishkg*x2Boost);
            SetPlayerChatBubble(playerid, fishstr, -1, 20.0, 5000);
		}
		else if(fish == 5)
        {
            FishingSlot[playerid] = 0;
            new fishkg = Random(2,3);
            PlayerInfo[playerid][pKalmaxi] += fishkg*x2Boost;
            PlayerUpdateData(playerid, "pKalmaxi", PlayerInfo[playerid][pKalmaxi]);
            format(fishstr, sizeof(fishstr), "{63BD4E}[Fishing System] {FFFFFF}Tkven Daichiret: {63BD4E}Kalmaxi {FFFFFF}/ {63BD4E}%d{FFFFFF}KG", fishkg*x2Boost);
            SendClientMessage(playerid, COLOR_WHITE, fishstr);

			SetPlayerAttachedObject(playerid, 8, 18632,6,0.079376,0.037070,0.007706,181.482910,0.000000,0.000000,1.000000,1.000000,1.000000);
            ApplyAnimation(playerid,"SWORD","sword_block",50.0,0,1,0,1,1);
            SetTimerEx("FishAnimation", 800, false, "i", playerid);

            format(fishstr, sizeof(fishstr), "Daichira: %dKG Kalmaxi", fishkg*x2Boost);
            SetPlayerChatBubble(playerid, fishstr, -1, 20.0, 5000);
    	}
        else if(fish == 6)
        {
            FishingSlot[playerid] = 0;
			SendClientMessage(playerid, COLOR_WHITE, "{63BD4E}[Fishing System] {FFFFFF}Samwuxarod Veraferi Ver Daichiret");

			SetPlayerAttachedObject(playerid, 8, 18632,6,0.079376,0.037070,0.007706,181.482910,0.000000,0.000000,1.000000,1.000000,1.000000);
            ApplyAnimation(playerid,"SWORD","sword_block",50.0,0,1,0,1,1);
            SetTimerEx("FishAnimation", 800, false, "i", playerid);
		}
    }
	return true;
}
publics: RandomKeyButter(playerid)
{
	new keytext[32],entkey;
