            new strr1[256];
            new Float:hppp;
            new Float:aermor;
            GetPlayerArmour(SpecAd[i],aermor);
            GetPlayerHealth(SpecAd[i],hppp);
            if(IsPlayerInAnyVehicle(SpecAd[i]))
            {
                new Float:health;
                GetVehicleHealth(GetPlayerVehicleID(SpecAd[i]),health);
                format(strr1, sizeof(strr1),"%s~n~(%d)~n~~n~~r~]Armour: ~w~%.0f~n~~g~]Hp: ~w~%.0f~n~~g~]CarHp: ~w~%.0f~n~~r~]Money: ~w~%d~n~~b~]Ammo: ~w~%d~n~~p~]Ping: ~w~%d~n~~b~]Speed: ~w~%d",Name(SpecAd[i]),i,aermor,hppp,health,PlayerInfo[SpecAd[i]][pCash],GetPlayerAmmo(SpecAd[i]),GetPlayerPing(SpecAd[i]),SpeedVehicle(SpecAd[i]));
                PlayerTextDrawSetString(i,FULLRECON[i],strr1);
            }
            else
            {
                format(strr1, sizeof(strr1),"%s~n~(%d)~n~~n~~r~]Armour: ~w~%.0f~n~~g~]Hp: ~w~%.0f~n~~g~]CarHp: ~w~%.0f~n~~r~]Money: ~w~%d~n~~b~]Ammo: ~w~%d~n~~p~]Ping: ~w~%d~n~~b~]Speed: ~w~%d",Name(SpecAd[i]),i,aermor,hppp,PlayerInfo[SpecAd[i]][pBank],PlayerInfo[SpecAd[i]][pCash],GetPlayerAmmo(SpecAd[i]),GetPlayerPing(SpecAd[i]),SpeedVehicle(SpecAd[i]));
                PlayerTextDrawSetString(i,FULLRECON[i],strr1);
            }
        }
        new vehicleid = GetPlayerVehicleID(i);
        if(Fuell[vehicleid] <= 0)
		{
			Fuell[vehicleid] = 0;
			GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
			SetVehicleParamsEx(vehicleid,VEHICLE_PARAMS_OFF,lights,alarm,doors,bonnet,boot,objective);
			zavodis[GetPlayerVehicleID(i)] = 0;
			GameTextForPlayer(i,"~r~FUEL HAS ENDED", 5000, 3);
		}
        if(PlayerInfo[i][pMuteTime] > 0) PlayerInfo[i][pMuteTime] -= 1;
        if(PlayerInfo[i][pMuteTime] == 0 && PlayerInfo[i][pMuted] == 1)
		{
			PlayerInfo[i][pMuted] = 0;
			SvMutePlayerDisable(i);
		 	SendClientMessage(i,COLOR_LIGHTRED, "Tven Mogexsnat Chatis Bani");
		}
        if(PlayerInfo[i][pJailed] > 0)
        {
            if(PlayerInfo[i][pJailTime] > 0)
            {
                PlayerInfo[i][pJailTime] -= 1;
            }
            if(PlayerInfo[i][pJailTime] <= 0)
            {
                if(PlayerInfo[i][pJailed] == 1)
                {
                    DisablePlayerCheckpoint(i);
                    SetPlayerSpecialAction(i, 0);
                    RemovePlayerAttachedObject(i,1);
                    SetPlayerInterior(i, 0), SetPlayerPos(i,1545.0535,-1675.7673,13.5596), SetPlayerFacingAngle(i, 267.9767);
                }
				else if(PlayerInfo[i][pJailed] == 4) SetPlayerWorldBounds(i,20000.0000,-20000.0000,20000.0000,-20000.0000), SetPlayerInterior(i, 0), SetPlayerPos(i, 90.2101,1920.4854,17.9422);
				PlayerInfo[i][pJailTime] = 0;
				PlayerUpdateData(i, "pJailTime", PlayerInfo[i][pJailTime]);
				PlayerInfo[i][pJailed] = 0;
				PlayerUpdateData(i, "pJailed", PlayerInfo[i][pJailed]);
				PlayerCuffed[i] = 0;
				SendClientMessage(i, COLOR_ISPOLZUY, "Tqven Moixadet Sasjeli Da Tavisupali Xart");
				PlayerInfo[i][pWantedLevel] = 0;
				SetPlayerWantedLevel(i, 0);
				SetPlayerVirtualWorld(i, 0);
				GameTextForPlayer(i,"~g~Svabodaa", 5000, 1);
                PlayerUpdateData(i, "pWantedLevel", PlayerInfo[i][pWantedLevel]);
				SpawnPlayer(i);
			}
		}
        if(PlayerTazeTime[i] >= 1) PlayerTazeTime[i] += 1;
        if(PlayerTazeTime[i] >= 15) PlayerTazeTime[i] = 0, TogglePlayerControllable(i, 1);
        if(PlayerCuffed[i] == 1)
        {
            if(PlayerCuffedTime[i] <= 0)
            {
                TogglePlayerControllable(i, 1);
                PlayerCuffed[i] = 0;
                PlayerCuffedTime[i] = 0;
                PlayerTazeTime[i] = 1;
            }
            else PlayerCuffedTime[i] -= 1;
        }
        if(PlayerCuffed[i] == 2)
        {
            if(PlayerCuffedTime[i] <= 0)
            {
                TogglePlayerControllable(i, 1);
                PlayerCuffed[i] = 0;
                PlayerCuffedTime[i] = 0;
            }
            else PlayerCuffedTime[i] -= 1;
        }
    }
    if ((hour > ghour) || (hour == 0 && ghour == 23))
    {
        format(string, sizeof(string), " {77D47E}Amjamad {ffffff}(%d:00) {77D47E}Saatia",hour);
        BroadCast(COLOR_WHITE,string);
        ghour = hour;
        PayDay();
		banksystem = 0;
		bankbom[0] = 1;
		bankbom[1] = 1;
		bankbom[2] = 1;
		bankbom[3] = 1;
		bankbom[4] = 1;
		bankbom[5] = 1;
		bankbom[6] = 1;
		bankbom[7] = 1;
		bankbom[8] = 1;
		bankbom[9] = 1;
        if(opn == 1)
        {
            for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
            {
                if(IsPlayerInRangeOfPoint(i,30,1990.21594, 1855.96594, 994.28479)) SetPlayerPos(i,-2691.8586,810.8699,1500.9688), SendClientMessage(i,-1,"Bankis sacavi agdgenilia");
            }
            gaxsnilia = 0;
            agebulia = 0;
            MoveObject(seifi, 1990.21594, 1855.96594, 994.28479, 50, 0.00000, 0.00000, 180.00000);
            bombis = Create3DTextLabel( "Bombis Dasadebad Daweret\n/setbomb", 0x008080FF, 1989.97754, 1855.77234, 993.18683, 20.0, 1);
        }
        if (realtime) SetWorldTime(hour);
	}
    if(hour == 04 && minute == 49 && second == 00)
	{
	    for(new i = 1; i <= TotalBizz; i++) SaveBizz(i);
    	for(new i = 1; i <= TotalHouse; i++) SaveProperty(i);
		SaveAccounts();
		SaveGZ();
        SaveFracBank();
	}
	if ((hour > ghour) || (hour == 0 && ghour == 23))
    {
        for(new i = 1; i <= TotalBizz; i++) SaveBizz(i);
        for(new i = 1; i <= TotalHouse; i++) SaveProperty(i);
        SaveAccounts();
        SaveGZ();
        SaveFracBank();
        SaveMZ();
        SaveKazino();
    }
    UpdateCasino();
    return true;
}
