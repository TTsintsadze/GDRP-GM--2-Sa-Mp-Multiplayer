		PlayerInfo[playerid][pTut] = 0;
		PlayerInfo[playerid][pChar][0] = 55;
		PlayerInfo[playerid][pModel] = 255;
		PlayerInfo[playerid][pMember] = 0;
		PlayerInfo[playerid][pLeader] = 0;
		PlayerInfo[playerid][pRank] = 1;
		ChosenSkin[playerid] = 55;
		SetPlayerInterior(playerid,0);
		for(new i = 0; i < 10; i++) PlayerTextDrawShow(playerid, skin_TD[playerid][i]);
		SelectTextDraw(playerid,0x4169e1FF);
		WC_TogglePlayerControllable(playerid, 0);
		SetPlayerSkin(playerid,PlayerInfo[playerid][pChar][0]);
		SelectChar[playerid] = 255;
