		}
	}
	return true;
}
public OnPlayerExitVehicle(playerid, vehicleid)
{
    SetPVarInt(playerid,"AFK_Time",0);
   // SetPVarInt(playerid, "GunCheckTime", 2);
    if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && TransportDuty[playerid] == 1)
    {
        Delete3DTextLabel(taxi3d[GetPlayerVehicleID(playerid)]);
		TransportDuty[playerid] = 0;
		SendClientMessage(playerid, 0x6BB3FFAA, "Tqven Daasrulet Samushao Dge.");
		return true;
	}
	if(gcontract[playerid] == 1)
	{
	    if(GetPlayerVehicleSeat(playerid) != 0) return true;
		Delete3DTextLabel(Meh3d[vehicleid]);
		cenabenza[playerid] = 0;
		gcontract[playerid] = 0;
		//SendClientMessage(playerid, -1, "Kontraqti Gauqmda!");
		return true;
	}
	if(GetPlayerState(playerid) == PLAYER_STATE_PASSENGER)
	{
		foreach(new i:Player)
		{
			if(IsPlayerInVehicle(i, GetPlayerVehicleID(playerid)) && GetPlayerState(i) == 2 && TransportDuty[i] == 1)
			{
				if(GetPVarInt(i,"Taxi_")-1 == playerid)
				{
					SendClientMessage(i, COLOR_BLUE, "Tkvenma Mgzavrma Datova Transporti, Tanxa Dagericxebat Payday'ze");
					if(GetPVarInt(i,"PayDayCashJob") > gettime()) {}
					else
					{
						new taxizp = 200+random(200);
						PlayerInfo[i][pPayCheck] += taxizp*PlayerInfo[i][ptaxilvl];
					}
					SetPVarInt(playerid, "TaxiPlayer", i);
					if(taxion[GetPVarInt(i,"Taxi_")-1] == false)
					{
						ShowPlayerDialog(GetPVarInt(i,"Taxi_")-1, 1203, DIALOG_STYLE_MSGBOX, "Taxi", "Mogewonat Taxis Momsaxureba?", "Ki", "Ara");
                        taxion[GetPVarInt(i,"Taxi_")-1] = true;
                        SetTimerEx("TaxiOFF", 60000, false, "i", GetPVarInt(i,"Taxi_")-1);
					}
					DeletePVar(i,"Taxi_");
					for(new id; id < GetMaxPlayers(); id++)
					{
						if(IsPlayerInVehicle(id, GetPlayerVehicleID(playerid)) && id != i && id != playerid)
						{
						    new taxistr[180];
							format(taxistr, sizeof(taxistr), "Miiyvanet Mgzavri: %s Danishnulebis Adgilas Da Xelfasi Payday'ze Dagericxebat",Name(id));
							SendClientMessage(i, COLOR_BLUE, taxistr);
							SetPVarInt(i,"Taxi_",id+1);
							break;
						}
					}
				}
			}
		}
	}
	new string[100];
	if(PEfir[playerid] != 255)
	{
		format(string, sizeof(string), "Tqven Gamoxvedit Pirdapiri Eteridan");
		SendClientMessage(playerid, COLOR_WHITE, string);
		PEfir[playerid] = 255;
	}
	if(Pefir[playerid] == 1 || Pefir[playerid] == 2)
	{
		PEfir[playerid] = 255;
		format(string, sizeof(string), "Tqven Gamoxvedit Pirdapiri Eteridan");
		SendClientMessage(playerid, COLOR_WHITE, string);
	}
	return 1;
}
public OnPlayerRequestClass(playerid, classid)
{
    if(IsPlayerNPC(playerid)) return 1;
	new pup[16];
	GetPlayerIp(playerid,pup,sizeof(pup));
	/* -------------------------------------------- DELETE FROM `banip` WHERE */
	new hgh[156];
	format(hgh, sizeof(hgh), "SELECT * FROM `banip` WHERE IP = '%s'",pup);
	mysql_tquery(connects, hgh, "CheckBanIP", "i",playerid);
	/* ---------------------------------------------------------------------- */
    new bac[512];
    if(PlayerInfo[playerid][pLogin] == 1) return SpawnPlayer(playerid);
    ResetNew(playerid);
	SetPlayerInterior(playerid, 0);
	SetPlayerPos(playerid, 1948.369262, 960.124877, 68.691299);
	SetPlayerVirtualWorld(playerid, playerid+2);
	SetPlayerCameraLookAt(playerid, 984.2333,-2017.0931,75.8742);
	TogglePlayerControllable(playerid,0);
	format(bac, sizeof(bac), "SELECT `Name` FROM `accounts` WHERE name = '%s'",Name(playerid));
	mysql_query(connects,bac);
	new rows;
	cache_get_row_count(rows);
 	if(rows)
	{
		format(autreg, sizeof(autreg), "{FFFFFF}Chaweret paroli, rata gaiarot avtorizacia.\n{FFCC00}• {FFFFFF}Paroli aucilebelia rata gaiarot avtorizacia.\n{ff0000}Paroli ar unda sheicavdes ucxo simboloebs");
		ShowPlayerDialog(playerid,1,DIALOG_STYLE_PASSWORD, "{1EAEEC}Avtorzacia",autreg, "Shesvla", "Options");
	}
	else
	{
		format(autreg, sizeof(autreg), "\
		{FFFFFF}Mogesalmebat chvens serverze {FFCC00}Green Dreams Role Play.\n\
		{FFFFFF}Rata daiwyot tamashi chvens serverze gtxovt gaiarot registracia.\n\n\
		{0E75EB}Gaitvaliswinet:\n\
		{DEE26F}- Parolis sigrdze unda iyos 6 - 32 simbolo.\n\
		{DEE26F}- Paroli sheyvanili unda iyos latinuri asoebit.\n\
		{DEE26F}- Paroli aucilebelia registraciistvis.");
		ShowPlayerDialog(playerid,2,DIALOG_STYLE_INPUT, "{1EAEEC}Registracia", autreg, "Shemdegi", "Gasvla");
    }
	return true;
