publics: update_speedometer(playerid)
{
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
    new vehicleid = GetPlayerVehicleID(playerid);
    new Float:Health, stext[20];
	GetVehicleHealth(vehicleid,Health);
	format(stext,sizeof(stext),"%d",SpeedVehicle(playerid));
	PlayerTextDrawSetString(playerid, speedometr[playerid][6], stext);
	format(stext,sizeof(stext),"%.0f L.",Fuell[vehicleid]);
	PlayerTextDrawSetString(playerid, speedometr[playerid][8], stext);
	if(Health > 400)
	{
		format(stext,sizeof(stext),"%d",floatround(Health));
		PlayerTextDrawSetString(playerid, speedometr[playerid][5], stext);
	}
	else
	{
		format(stext,sizeof(stext),"%d",floatround(Health));
		PlayerTextDrawSetString(playerid, speedometr[playerid][5], stext);
	}
	new Float:sx, Float:sy, Float:sz;
	GetPlayerPos(playerid, sx,sy,sz);
	new mandzili = floatround(GetDistanceBetweenPoints(cordinats[playerid][0],cordinats[playerid][1],cordinats[playerid][2], sx,sy,sz), floatround_round);
	if(mandzili > 200)
	{
	    kilometrs[playerid]++;
		GetPlayerPos(playerid, cordinats[playerid][0],cordinats[playerid][1],cordinats[playerid][2]);
	}
	format(stext,sizeof(stext),"%d KM",kilometrs[playerid]);
	PlayerTextDrawSetString(playerid, speedometr[playerid][7], stext);
	new Float:X, Float:Y, Float:Z;
	GetVehicleVelocity(vehicleid,X,Y,Z);
	new Float:speed = floatsqroot(floatadd(floatadd(floatpower(X, 2), floatpower(Y, 2)),  floatpower(Z, 2))) * 100.3;
	new lims[64];
	if(PlayerInfo[playerid][pSLimit])
	{
		format(lims,sizeof(lims),"(%d)",PlayerInfo[playerid][pSLimit]);
		if(speed > PlayerInfo[playerid][pSLimit])
		{
			new Float:dif = PlayerInfo[playerid][pSLimit] / speed;
			SetVehicleVelocity(vehicleid,X*dif,Y*dif,Z);
			speed = PlayerInfo[playerid][pSLimit];
		}
	}
	if(Fuell[vehicleid] <= 0 && GetVehicleModel(vehicleid) != 462 && GetVehicleModel(vehicleid) != 510 && GetVehicleModel(vehicleid) != 481)
	{
	    if(SpeedVehicle(playerid) >= 50)
		{
		    if(antispeed[playerid] == false)
		    {
				format(anticheat, sizeof(anticheat), "[AntiCheat] Motamashe: %s Shesadzloa Iyenebdes SpeedHack's | /recon [%d]", Name(playerid), playerid);
		    	SendAdminMessage(COLOR_LIGHTRED, anticheat);
		    	TecAdminMessage(COLOR_LIGHTRED, anticheat, 1);
				antispeed[playerid] = true;
				SetTimerEx("speedoff",20000,false,"d",playerid);
			}
		}
	}
	if(GetPlayerVehicleID(playerid) >= AutoSalon[0] && GetPlayerVehicleID(playerid) <= AutoSalon[24] && SpeedVehicle(playerid) > 5)
	{
	    if(antidialog[playerid] == false)
	    {
			format(anticheat, sizeof(anticheat), "[AntiCheat] Motamashe: %s Shesadzloa Iyenebdes Dialog Hack's | /recon [%d]", Name(playerid), playerid);
	    	SendAdminMessage(COLOR_LIGHTRED, anticheat);
	    	TecAdminMessage(COLOR_LIGHTRED, anticheat, 1);
			antidialog[playerid] = true;
			SetTimerEx("dialoghack",20000, false, "d", playerid);
		}
	}
	return 1;
}
