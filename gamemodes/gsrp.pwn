CMD:limit(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_REDD, "[Daxmareba]: /limit [Sichqare 10 - 150]");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid,COLOR_WHITE,"Tqven Arzicart Rulze");
	if(!params[0])
	{
		if(PlayerInfo[playerid][pSLimit])
		{
			PlayerInfo[playerid][pSLimit] = 0;
			SendClientMessage(playerid,0x7FC0E3FF,"Tqven Airchiet Tqveni Sichqaris Moculoba");
		}
		else SendClientMessage(playerid,COLOR_WHITE,"/limit [Sichqare]");
		return 1;
	}
	if(params[0] < 10 || params[0] > 150) return SendClientMessage(playerid,COLOR_WHITE,"Sichqaris Moculoba Unda SHeadgendes 10 - 150");
	PlayerInfo[playerid][pSLimit] = params[0];
	SendClientMessage(playerid,0x7FC0E3FF,"SpeedLimit Dayenda");
	return true;
}
