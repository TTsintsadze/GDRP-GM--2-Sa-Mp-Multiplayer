	{
		AddVehicleComponent (vehicleid, PlayerInfo[playerid][pWheels][0]);
		AddVehicleComponent (vehicleid, PlayerInfo[playerid][pNitro][0]);
		AddVehicleComponent (vehicleid, PlayerInfo[playerid][pHydrawlic][0]);
		if(PJ(vehicleid))
		{
			ChangeVehiclePaintjob(vehicleid, PlayerInfo[playerid][pPaintJob][0]);
			AddVehicleComponent (vehicleid, PlayerInfo[playerid][pSpoiler][0]);
			AddVehicleComponent (vehicleid, PlayerInfo[playerid][pBumper1][0]);
