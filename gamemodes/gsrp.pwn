CMD:transfer(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(GetPVarInt(playerid, "StopFlood") > 0) return SendClientMessage(playerid, COLOR_REDD, "|STOP FLOOD|{FFFFFF} Brdzanebis Gamoyeneba Dashvebulia 3-Wamshi Ertxel!");
    if(PlayerInfo[playerid][pLevel] < 5) return infotxt(playerid, "Am CMD's Gamosayeneblad Sachiroa Iyot 5Level!");
	if(!IsABank(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Tqven Ar Shegidzlia Am CMD's, Radgan Tqven Ar Imyofebit Bankshi");
	if(sscanf(params, "ud", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /transfer [ID Motamashis] [Tanxa]");
	if (!IsPlayerConnected(params[0])) return true;
	if(params[1] > 0 && PlayerInfo[playerid][pBank] >= params[1])
	{
	    BizzInfo[26][bTill] += params[1] / 15;
		PlayerInfo[playerid][pBank] -= params[1];
		PlayerInfo[params[0]][pBank] += params[1]-params[1] / 15;
		format(YCMDstr, sizeof(YCMDstr), "[Bank Transfer] Tkven Gadauricxet Motamashe: {63BD4E}%s{FFFFFF}'s {63BD4E}%d$ {FFFFFF}/ Sakomisio: {63BD4E}%d$", Name(params[0]), params[1]-params[1] / 15, params[1] / 15);
		SendClientMessage(playerid, COLOR_WHITE, YCMDstr);
		format(YCMDstr, sizeof(YCMDstr), "[Bank Transfer] %s Gadmogiricxat %d$ Tqvens Angarishze", Name(playerid), params[1]-params[1] / 15);
		SendClientMessage(params[0], COLOR_WHITE, YCMDstr);
		PlayerPlaySound(params[0], 1052, 0.0, 0.0, 0.0);
		SetPVarInt(playerid, "StopFlood", 3);
	    PlayerUpdateData(playerid, "pCash", PlayerInfo[playerid][pCash]);
	    PlayerUpdateData(playerid, "pBank", PlayerInfo[playerid][pBank]);
    	PlayerUpdateData(params[0], "pBank", PlayerInfo[params[0]][pBank]);
    	if(params[1] >= 5000000)
    	{
			new year, month,day, hour, minuite, second, cmdstr[40], cmdlogs[300], data[100], time[80];
			getdate(year, month, day);
			format(data,sizeof(data), "%d/%d/%d", day, month, year);
			gettime(hour,minuite,second);
			if(minuite < 10)
			{
				format(time, sizeof(time), "%d:0%d", hour, minuite);
			}
			else
			{
				format(time, sizeof(time), "%d:%d", hour, minuite);
			}
			format(cmdstr, sizeof(cmdstr), "/transfer");
			format(cmdlogs, sizeof(cmdlogs), "INSERT INTO `MoneyLog` (`CMD`, `Name`, `Recipient`, `Money`, `Data`, `Time`) VALUE ('%s', '%s', '%s', '%d', '%s', '%s')", cmdstr, Name(playerid), Name(params[0]), params[1], data, time);
			mysql_query(connects,cmdlogs);
    	}
	}
	return true;
}
